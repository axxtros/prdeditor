<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PrdEditor"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- system -->
    <system:String x:Key="applicationName">Dictionary Table Editor (Development Tool)</system:String>

    <!-- main menu -->
    <system:String x:Key="mainMenu_File">File</system:String>
    <system:String x:Key="mainMenu_Edit">Edit</system:String>
    <system:String x:Key="mainMenu_View">View</system:String>
    <system:String x:Key="mainMenu_Window">Window</system:String>
    <system:String x:Key="mainMenu_Help">Help</system:String>
    
    <!-- toolbar -->
    <system:String x:Key="newConnectionButton">New connection</system:String>
    <system:String x:Key="newConnectionButtonTooltip">Create new database connection</system:String>

</ResourceDictionary>